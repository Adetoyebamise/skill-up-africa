<h1>Books Page | Search Book</h1>
<h2>Search Books</h2>
<label>Title</label>
<form action="/books" method="GET">
  <input type="text" name="title" value="<%= searchOption.title%>"/>
  <button type="submit">search</button>
  <label>Published After</label>
  <input
    type="date"
    name="publishedAfter"
    value="<%= searchOption.publishedAfter%>"
  />
  <label>Published Before</label>
  <input
    type="date"
    name="publishedBefore"
    value="<%= searchOption.publishedBefore%>"
  />
  <button type="submit">search</button>
</form>
  <br />
  <div>
  <% books.forEach(book => { %>
  <img height="150" width="100" src="<%= book.coverImagePath %>
  <% })%>
</div>

